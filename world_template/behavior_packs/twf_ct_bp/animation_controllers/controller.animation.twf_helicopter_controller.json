{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.twf_helicopter.life": {
			"initial_state": "default",
			"states": {
				"default": {
                    "on_entry": [
                    ],
                    "transitions": [
                        {
                            "crane": "math.round(math.random(1, 1000)) == 1"
                        },
                        {
                            "park": "math.round(math.random(1, 1000)) == 1"
                        },
                        {
                            "default": "true"
                        }
                    ]
				},
                "crane": {
                    "on_entry": [
                            "/say ยง2Deploying a new crane!",
                            "/summon twf:crane ~ ~0.1 ~",
                            "/particle twf:rainbow_flame ~ ~ ~",
                            "/kill @s"
                    ],                    
                    "transitions": [
                        {
                            "default": "true"
                        }
                    ]
				},
                "park": {
                    "on_entry": [
                            "/say ยง2Deploying a new park!",
                            "/summon twf:park1 ~ ~0.1 ~",
                            "/particle twf:rainbow_flame ~ ~ ~",
                            "/kill @s"
                    ],                    
                    "transitions": [
                        {
                            "default": "true"
                        }
                    ]
				}
                
            }
		}
	}
}